---
layout: null
---
[
  {% assign static_pages = site.pages | where_exp: "item", "item.title != nil" | where_exp: "item", "item.noindex != true" %}
  {% assign all_items = site.posts | concat: static_pages %}
  
  {% for item in all_items %}
    {
      "title": {{ item.title | jsonify }},
      "url": {{ item.url | relative_url | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]